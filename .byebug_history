exit
        @coupons= Coupon.create(store: params[:coupon][:store], coupon_code: params[:coupon][:coupon_code])
        @coupons= Coupon.create(store: params[coupon][:store], coupon_code: params[coupon][:coupon_code])
        @coupons= Coupon.create(store: params[:store], coupon_code: params[:coupon_code])
@coupon
params
